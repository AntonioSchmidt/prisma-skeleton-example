{
  "name": "TiketLiveBackend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "12.x.x"
  },
  "scripts": {
    "start:heroku": "node dist",
    "start": "yarn build && node dist",
    "clean": "rm -rf dist",
    "build": "yarn clean && yarn generate generate && ts-node --transpile-only src/schema && tsc",
    "generate": "node_modules/.bin/prisma generate",
    "migrate:up": "node_modules/.bin/prisma migrate up --experimental ",
    "migrate:save": "node_modules/.bin/prisma migrate save --experimental ",
    "postinstall": "yarn generate && yarn migrate:up",
    "start:dev": "node_modules/.bin/prisma generate --watch & ts-node-dev --respawn --transpile-only src/schema & tsc-watch --onSuccess \"node dist\""
  },
  "dependencies": {
    "@nexus/schema": "^0.13.1",
    "@prisma/client": "^2.0.0-beta.3",
    "apollo-server": "^2.12.0",
    "expo-cli": "^3.18.2",
    "global": "^4.4.0",
    "graphql": "^15.0.0",
    "graphql-shield": "^7.2.6",
    "nexus-prisma": "^0.12.0",
    "redis": "^3.0.2",
    "sqlite3": "^4.1.1",
    "uuidv4": "^6.0.9",
    "ws": "^7.2.5"
  },
  "devDependencies": {
    "@prisma/cli": "^2.0.0-beta.3",
    "@types/node": "^13.11.1",
    "@types/redis": "^2.8.20",
    "ts-node": "^8.8.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.8.3"
  }
}
